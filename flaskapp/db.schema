CREATE TABLE team (
	id INTEGER NOT NULL,
	name VARCHAR(20) NOT NULL,
	image_file VARCHAR(20) NOT NULL,
	PRIMARY KEY (id),
	UNIQUE (name),
	UNIQUE (image_file)
);

